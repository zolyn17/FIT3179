{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "title": "World Suicide Cases Per 100k Population",
  "width": 720,
  "height": 350,
  "data": {
    "url": "https://raw.githubusercontent.com/KaneSec/vega_lite/main/3_choropleth_map/js/ne_110m_admin_0_countries.topojson",
    "format": {"type": "topojson", "feature": "ne_110m_admin_0_countries"}
  },
  "projection": {"type": "equalEarth"},
  "layer": [
    {
      "transform": [
        {
          "calculate": "'Data is not available in ' + datum.properties.NAME",
          "as": "note"
        }
      ],
      "mark": {"type": "geoshape", "fill": "lightgray", "stroke": "white"},
      "encoding": {"tooltip": {"field": "note"}}
    },
    {
      "transform": [
        {
          "lookup": "properties.NAME",
          "from": {
            "data": {
              "url": "https://raw.githubusercontent.com/zolyn17/FIT3179/main/CSV/Map_data_world_suicide.csv"
            },
            "key": "Country",
            "fields": ["Suicides", "Population"]
          }
        },
        {
          "calculate": "datum.Suicides/datum.Population * 100000",
          "as": "Suicide Rate"
        }
      ],
      "mark": {"type": "geoshape", "stroke": "white"},
      "encoding": {
        "color": {
          "field": "Suicide Rate",
          "type": "quantitative",
          "scale": {
            "type": "threshold",
            "domain": [1, 10, 20],
            "range": ["#fdbe85", "#fd8d3c", "#e6550d", "#a63603"]
          }
        },
        "tooltip": [
          {"field": "properties.NAME", "type": "nominal", "title": "Country"},
          {"field": "Suicides", "type": "quantitative"},
          {"field": "Population", "type": "quantitative"},
          {"field": "Suicide Rate", "type": "quantitative"}
        ]
      }
    },
    {
      "transform": [{"filter": "datum.properties.NAME== 'Canada'"}],
      "mark": {
        "type": "text",
        "font": "Open Sans",
        "x": 120,
        "y": 30,
        "fontWeight": "bold",
        "color": "#8B0000",
        "fontSize": 10
      },
      "encoding": {"text": {"value": "Canada"}}
    },
    {
      "transform": [{"filter": "datum.properties.NAME== 'Brazil'"}],
      "mark": {
        "type": "text",
        "font": "Open Sans",
        "x": 300,
        "y": 220,
        "fontWeight": "bold",
        "color": "#8B0000",
        "fontSize": 10
      },
      "encoding": {"text": {"value": "Brazil"}}
    },
    {
      "transform": [{"filter": "datum.properties.NAME== 'Australia'"}],
      "mark": {
        "type": "text",
        "font": "Open Sans",
        "x": 670,
        "y": 220,
        "fontWeight": "bold",
        "color": "#8B0000",
        "fontSize": 10
      },
      "encoding": {"text": {"value": "Australia"}}
    },
    {
      "transform": [{"filter": "datum.properties.NAME== 'Mongolia'"}],
      "mark": {
        "type": "text",
        "font": "Open Sans",
        "x": 550,
        "y": 40,
        "fontWeight": "bold",
        "color": "#8B0000",
        "fontSize": 10
      },
      "encoding": {"text": {"value": "Mongolia"}}
    },
    {
      "transform": [{"filter": "datum.properties.NAME== 'Mexico'"}],
      "mark": {
        "type": "text",
        "font": "Open Sans",
        "x": 120,
        "y": 100,
        "fontWeight": "bold",
        "color": "#8B0000",
        "fontSize": 10
      },
      "encoding": {"text": {"value": "Mexico"}}
    },
    {
      "transform": [{"filter": "datum.properties.NAME== 'Argentina'"}],
      "mark": {
        "type": "text",
        "font": "Open Sans",
        "x": 280,
        "y": 270,
        "fontWeight": "bold",
        "color": "#8B0000",
        "fontSize": 10
      },
      "encoding": {"text": {"value": "Argentina"}}
    },
    {
      "transform": [{"filter": "datum.properties.NAME== 'Kazakhstan'"}],
      "mark": {
        "type": "text",
        "font": "Open Sans",
        "x": 470,
        "y": 30,
        "fontWeight": "bold",
        "color": "#8B0000",
        "fontSize": 10
      },
      "encoding": {"text": {"value": "Kazakhstan"}}
    }
  ],
  "config": {}
}